{
    "name": "apn-ra/asterisk-pbx-manager",
    "description": "Comprehensive Laravel package for Asterisk PBX management via AMI with event processing, queue management, and real-time monitoring",
    "type": "library",
    "keywords": ["laravel", "asterisk", "pbx", "ami", "pami", "voip", "telephony", "call-center", "queue-management", "real-time", "events"],
    "license": "MIT",
    "homepage": "https://github.com/apn-ra/asterisk-pbx-manager",
    "readme": "README.md",
    "authors": [
        {
            "name": "APN RA",
            "email": "ronniel.castanito@apntelecom.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/apn-ra/asterisk-pbx-manager/issues",
        "source": "https://github.com/apn-ra/asterisk-pbx-manager",
        "docs": "https://github.com/apn-ra/asterisk-pbx-manager/blob/main/README.md"
    },
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/config": "^12.0",
        "illuminate/events": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/log": "^12.0",
        "illuminate/broadcasting": "^12.0",
        "marcelog/pami": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5 || ^12.0",
        "orchestra/testbench": "^10.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "AsteriskPbxManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsteriskPbxManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AsteriskPbxManager\\AsteriskPbxManagerServiceProvider"
            ],
            "aliases": {
                "AsteriskManager": "AsteriskPbxManager\\Facades\\AsteriskManager"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}